{
	"name": "react-isomorphism",
	"version": "0.0.1",
	"description": "webpack + babel + react + jest + eslint 脚手架",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "node app.js",
		"dev": "node app.js dev-server",
		"update": "node ./server/util/seolink.js",
		"build": "webpack --config ./webpack/webpack.pro.config.js && webpack --config ./webpack/webpack.server.config.js && npx tsc --outFile ./dist/server/notegroup.js ./menu/notegroup.ts",
		"lint": "eslint src --fix --ext .ts --ext .tsx ",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.{js,css,less,ts,tsx,md}": "prettier --write"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xmyxm/web-little.git"
	},
